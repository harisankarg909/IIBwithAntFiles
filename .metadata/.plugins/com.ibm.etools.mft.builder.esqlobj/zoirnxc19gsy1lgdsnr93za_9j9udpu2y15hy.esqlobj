CREATE PROCEDURE XferAdd_FlowInit_AddComponentDetails()
BEGIN
    DECLARE C_FILE CONSTANT CHAR 'XferAdd.esql';
    DECLARE C_REVISION CONSTANT CHAR '@CIMCANONICALVERSION@';
    DECLARE C_REVISIONDATE CONSTANT CHAR '@SCMCANONICALVERSIONDATE@';

    CALL CcnFlowInit_AddComponentDetails(C_FILE, C_REVISION, C_REVISIONDATE);
END;